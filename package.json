{
    "name": "laravel-blade-spacer",
    "displayName": "Laravel Blade Spacer",
    "description": "Automatically adds spaces in Laravel blade template tags",
    "version": "0.1.0",
    "publisher": "austenc",
    "repository": "https://github.com/austenc/vscode-blade-spacer",
    "icon": "img/icon.png",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}